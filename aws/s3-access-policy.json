{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::ai-interviewer-static-assets",
        "arn:aws:s3:::ai-interviewer-static-assets/*",
        "arn:aws:s3:::ai-interviewer-logs",
        "arn:aws:s3:::ai-interviewer-logs/*"
      ]
    }
  ]
} 